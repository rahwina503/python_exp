    "# for문 뒤에 if 조건문을 추가할 수도 있다.\n",
    "\n",
    "a = [[1,11],[2,22],[3,33],[4,44],[5,55]]\n",
    "x = [i[0] for i in a if i[0] > 3]\n",
    "y = [i[1] for i in a if i[1] % 2 != 0]\n",
    "\n",
    "print(x)\n",
    "print(y)"
   ]
